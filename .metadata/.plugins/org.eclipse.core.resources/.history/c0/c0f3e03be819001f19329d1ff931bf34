package jp.fixie.test;

public class AssignmentArray3 {
	
	// 大問3 - 配列
	// 小問3
	public static void main(String[] args) {
		System.out.println("配列-小問3");
		String [][] array3 = {
				{"Java","研修"},
				{"多次元","配列"}
		};
		
		String [][] array4 = new String[2][3];
		array4[0][0] = "Java";
		array4[0][1] = "研修";
		array4[1][0] = "二次元";
		array4[1][1] = "配列";
		

	}

}
