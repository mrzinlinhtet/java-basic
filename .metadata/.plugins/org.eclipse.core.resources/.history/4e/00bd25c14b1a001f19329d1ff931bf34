package jp.fixie.test;

import java.util.List;

public class AssignmentList1 {
	
	public static void main(String[] args) {
		
		List arrayList1;

	    // Step 2: Call the argumentless constructor of java.util.ArrayList and store the instance in [arrayList1]
	    arrayList1 = new ArrayList();

	    // Step 3: Insert the number [1] at the end of the element of [arrayList1]
	    arrayList1.add(1);

	    // Step 4: Insert the string ["Java"] at the end of the element of [arrayList1]
	    arrayList1.add("Java");

	    // Step 5: Insert the number [1.5] at the end of the element of [arrayList1]
	    arrayList1.add(1.5);

	    // Step 6: Output the value of variable [arrayList1] to the console
	    System.out.println(arrayList1);

	}
}
